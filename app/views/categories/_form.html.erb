<%= form_with(url: categories_new_path,local: true) do |form| %>
  

  <div class="form-group">    
    <%= form.text_field :category_name, class: 'form-control', placeholder: 'Create New Category' %>
  </div>

  
  <div class="form-group">
    <%= form.submit 'Submit', class: 'btn btn-default btn-primary' %>
  </div>
<% end %>